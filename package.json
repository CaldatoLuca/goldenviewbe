{
  "name": "goldenviewbe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/index.js",
    "emulate": "cross-env NODE_ENV=production node dist/index.js",
    "prod": "npm run build && npm run start",
    "prisma:pull:dev": "dotenv -e .env.development -- npx prisma db pull",
    "prisma:pull:prod": "dotenv -e .env.production -- npx prisma db pull",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "prisma:studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "prisma:studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "prisma:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset --force",
    "prisma:reset:prod": "dotenv -e .env.production -- npx prisma migrate reset --force"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.9",
    "bcrypt-ts": "^8.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "prisma": "^7.2.0",
    "typescript": "^5.9.3",
    "uploadthing": "^7.7.4",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "tsx": "^4.21.0"
  }
}
